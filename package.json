{
	"name": "discord.js-bot-framework",
	"version": "2.0.0",
	"description": "A framework to easily built discord bots with inbuilt commands.",
	"main": "build/index.js",
	"scripts": {
		"dev": "rm -r ./build && tsc && node ./build/index.js",
		"build": "tsc",
		"start": "node ./build/index.js",
		"generate-doc": "jsdoc ./build -r -p -d docs -t node_modules/docdash",
		"generate-doc-notheme": "jsdoc ./build -r -p -d docs",
		"lint": "eslint --ext .ts ."
	},
	"devDependencies": {
		"@prisma/client": "^4.14.1",
		"@types/glob": "8.1.0",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"docdash": "2.0.1",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"jsdoc": "^4.0.2",
		"prettier": "^2.8.8",
		"typescript": "5.0.4"
	},
	"dependencies": {
		"croner": "^6.0.3",
		"discord.js": "^14.11.0",
		"glob": "^10.2.2",
		"prisma": "^4.14.1",
		"tsyringe": "^4.7.0"
	}
}
